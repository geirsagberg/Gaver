{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
    {
      "label": "typescript",
      "type": "shell",
      "command": "node_modules/typescript/bin/tsc --noEmit",
      "problemMatcher": ["$tsc"],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "dotnet build",
      "type": "shell",
      "command": "dotnet build",
      "problemMatcher": ["$msCompile"],
      "group": "build"
    }
  ]
}
