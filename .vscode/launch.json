{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceRoot}/src/Gaver.Web/bin/Debug/netcoreapp1.0/Gaver.Web.dll",
      "args": [],
      "cwd": "${workspaceRoot}/src/Gaver.Web",
      "stopAtEntry": false,
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    {
      "name": "Debug in Chrome",
      "type": "chrome",
      "request": "attach",
      "url": "http://localhost:5000/",
      "webRoot": "${workspaceRoot}/src/Gaver.Web/wwwroot",
      "sourceMaps": true,
      "port": 9222
    }
  ]
}