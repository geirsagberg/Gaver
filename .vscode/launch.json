{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (console)",
      "type": "coreclr",
      "request": "launch",
      "program": "${workspaceRoot}/src/Gaver.Web/bin/Debug/netcoreapp2.2/Gaver.Web.dll",
      "args": [],
      "cwd": "${workspaceRoot}/src/Gaver.Web",
      "stopAtEntry": false,
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:pickProcess}"
    },
    {
      "type": "chrome",
      "request": "attach",
      "name": "Attach to Chrome",
      "port": 9222,
      "webRoot": "${workspaceRoot}/src/Gaver.Web",
      "urlFilter": "http://localhost:5000/*",
      "sourceMaps": true,
      "smartStep": true,
      "skipFiles": ["node_modules/**"],
      "sourceMapPathOverrides": {
        "webpack:///*": "${webRoot}/*"
      }
    },
    {
      "name": "Chrome (Launch)",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:5000/login",
      "webRoot": "${workspaceRoot}/src/Gaver.Web/wwwroot",
      "sourceMaps": true,
      "port": 9222
    }
  ]
}
