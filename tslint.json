{
  "extends": ["tslint-config-standard"],
  "rules": {
    "quotemark": [true, "single", "jsx-double"],
    "one-variable-per-declaration": false,
    "ter-indent": false,
    "space-before-function-paren": false,
    "no-console": {
      "severity": "warning",
      "options": ["debug", "info", "log", "time", "timeEnd", "trace"]
    },
    "no-invalid-this": true
  }
}
