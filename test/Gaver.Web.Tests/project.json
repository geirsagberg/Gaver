{
  "version": "1.0.0-*",
  "buildOptions": {
    "debugType": "portable",
    "copyToOutput": {
      "include": [
        "xunit.runner.json"
      ]
    }
  },
  "dependencies": {
    "Microsoft.NETCore.App": {
      "type": "platform",
      "version": "1.0.1"
    },
    "System.Runtime.Serialization.Primitives": "4.1.1",
    "xunit": "2.2.0-*",
    "FluentAssertions": "4.9.1",
    "dotnet-test-xunit": "2.2.0-*",
    "Gaver.Web": {
      "target": "project"
    }
  },
  "testRunner": "xunit",
  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dotnet5.4",
        "portable-net451+win8"
      ]
    }
  }
}