{
  "version": "1.0.0-*",
  "testRunner": "xunit",
  "dependencies": {
    "dotnet-test-xunit": "2.2.0-*",
    "Microsoft.NETCore.App": {
      "type": "platform",
      "version": "1.0.1"
    },
    "xunit": "2.2.0-*",
    "Gaver.Data": {
      "target": "project"
    },
    "FluentAssertions": "4.9.1"
  },
  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dnxcore50",
        "portable-net45+win8"
      ]
    }
  },
  "buildOptions": {
    "debugType": "portable",
    "copyToOutput": {
      "include": [
        "xunit.runner.json"
      ]
    }
  },
  "tooling": {
    "defaultNamespace": "Gaver.Data.Tests"
  }
}