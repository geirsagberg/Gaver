{
  "version": "1.0.0-*",
  "buildOptions": {
    "emitEntryPoint": true,
    "preserveCompilationContext": true,
    "compile": {
      "include": ["**/*.cs"],
      "exclude": ["node_modules", "obj"]
    }
  },
  "runtimeOptions": {
    "gcServer": true
  },
  "tooling": {
    "defaultNamespace": "Gaver.Web"
  },
  "userSecretsId": "Gaver-d5fca685-5997-4e8a-9069-0750a83f161e",
  "tools": {
    "Microsoft.DotNet.Watcher.Tools": {
      "version": "1.0.0-*"
    },
    "Microsoft.EntityFrameworkCore.Tools": "1.0.0-preview2-final",
    "Microsoft.Extensions.SecretManager.Tools": "1.0.0-preview2-final"
  },
  "dependencies": {
    "Microsoft.NETCore.App": {
      "version": "1.0.0-*",
      "type": "platform"
    },
    "Gaver.Data": {
      "target": "project"
    },
    "Gaver.Logic": {
      "target": "project"
    },
    "Microsoft.AspNetCore.Diagnostics": "1.0.0-*",
    "Microsoft.AspNetCore.Server.IISIntegration": "1.0.0-*",
    "Microsoft.AspNetCore.Mvc": "1.0.0-*",
    "Microsoft.AspNetCore.Mvc.TagHelpers": "1.0.0-*",
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.0-*",
    "Microsoft.AspNetCore.StaticFiles": "1.0.0-*",
    "Microsoft.AspNetCore.SignalR.Server": "0.1.0-*",
    "Microsoft.AspNetCore.WebSockets.Server": "0.1.0",
    "Microsoft.Extensions.Configuration.Json": "1.0.0-*",
    "Microsoft.Extensions.Configuration.UserSecrets": "1.0.0-*",
    "Microsoft.Extensions.Logging.Console": "1.0.0-*",
    "Microsoft.Extensions.Logging.Filter": "1.0.0",
    "Microsoft.Extensions.Options.ConfigurationExtensions": "1.0.0-*",
    "Microsoft.NETCore.Platforms": "1.0.1-*",
    "Microsoft.Extensions.Logging.Debug": "1.0.0-*",
    "Microsoft.AspNetCore.ReactServices": "1.0.0-*",
    "Microsoft.EntityFrameworkCore.Sqlite": "1.0.0",
    "Microsoft.EntityFrameworkCore.Design": "1.0.0-preview2-final",
    "WebApiContrib.Core": "1.0.0",
    "Swashbuckle": "6.0.0-beta902",
    "MediatR": "2.1.0",
    "LightInject.Microsoft.DependencyInjection": "1.0.1",
    "Serilog": "2.1.0",
    "Serilog.Extensions.Logging": "1.2.0",
    "Serilog.Sinks.Literate": "2.0.0",
    "Serilog.Sinks.ColoredConsole": "2.0.0"
  },
  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dotnet5.6",
        "dnxcore50",
        "portable-net45+win8"
      ]
    }
  },
  "publishOptions": {
    "exclude": [
      "node_modules",
      "**.xproj",
      "**.user",
      "**.vspscc"
    ]
  },
  "scripts": {
    "prepare": [
      "npm install",
      "webpack --config webpack.config.vendor.js"
    ],
    "prepublish": [
      "webpack"
    ],
    "postpublish": [
      "dotnet publish-iis --publish-folder %publish:OutputPath% --framework %publish:FullTargetFramework%"
    ]
  }
}