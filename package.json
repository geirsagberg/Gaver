{
  "name": "gaver",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "foreach -g 'test/**Tests/*.csproj' -x 'cd #{dir} && dotnet test' --no-C",
    "test:watch": "npm test && chokidar '**/*.cs' -c 'npm test'",
    "clean": "del-cli src/**/obj/ src/**/bin/ test/**/obj/ test/**/bin/ node_modules/.cache/ src/**/dist",
    "clean:www": "del-cli src/Gaver.Web/wwwroot/dist/",
    "start": "cd src/Gaver.Web && cross-env ASPNETCORE_ENVIRONMENT=Development dotnet run",
    "start:prod": "npm run webpack && dotnet run --project src/Gaver.Web --launch-profile Production",
    "watch": "cd src/Gaver.Web && cross-env ASPNETCORE_ENVIRONMENT=Development dotnet watch run --urls \"https://0.0.0.0:5001;http://0.0.0.0:5000\"",
    "build": "dotnet build",
    "webpack": "cd src/Gaver.Web && webpack --mode production",
    "webpack:dev": "cd src/Gaver.Web && cross-env ASPNETCORE_ENVIRONMENT=Development webpack --mode development",
    "cloc": "cloc --exclude-dir=node_modules,typings,wwwroot,bin,obj --exclude-lang=JSON ."
  },
  "author": "Geir Sagberg",
