{
  "name": "gaver",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "foreach -g 'test/**/project.json' -x 'cd #{dir} && dotnet test'",
    "test:watch": "npm test && watch -p '**/*.cs' -c 'npm test'",
    "start": "cd src/Gaver.Web && cross-env ASPNETCORE_ENVIRONMENT=Development dotnet run",
    "watch": "cd src/Gaver.Web && cross-env ASPNETCORE_ENVIRONMENT=Development dotnet watch run",
    "build": "dotnet build **/project.json"
  },
  "author": "Geir Sagberg",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "eslint": "^3.2.2",
    "eslint-config-standard": "^5.3.5",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "eslint-plugin-standard": "^2.0.0",
    "foreach-cli": "^1.2.2",
    "watch-cli": "^0.2.1"
  },
  "dependencies": {
    "cross-env": "^2.0.0",
    "scriptjs": "^2.5.8"
  }
}
