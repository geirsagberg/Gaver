{
  "name": "gaver",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "foreach -g 'test/**/project.json' -x 'cd #{dir} && dotnet test'",
    "test:watch": "npm test && watch -p '**/*.cs' -c 'npm test'",
    "start": "cd src/Gaver.Web && ASPNETCORE_ENVIRONMENT=Development dotnet run",
    "watch": "cd src/Gaver.Web && ASPNETCORE_ENVIRONMENT=Development dotnet watch run",
    "build": "dotnet build **/project.json"
  },
  "author": "Geir Sagberg",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "eslint": "^2.11.1",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-promise": "^1.3.2",
    "eslint-plugin-react": "^5.1.1",
    "eslint-plugin-standard": "^1.3.2",
    "foreach-cli": "^1.0.6",
    "watch-cli": "^0.2.1"
  }
}
