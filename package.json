{
  "name": "gaver",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "foreach -g 'test/**/project.json' -x 'cd #{dir} && dotnet test'",
    "test:watch": "npm test && watch -p '**/*.cs' -c 'npm test'",
    "clean": "rimraf src/**/obj/ && rimraf src/**/bin/",
    "start": "cd src/Gaver.Web && cross-env ASPNETCORE_ENVIRONMENT=Development dotnet run",
    "start:prod": "cd src/Gaver.Web && webpack && dotnet run",
    "watch": "cd src/Gaver.Web && cross-env ASPNETCORE_ENVIRONMENT=Development dotnet watch run",
    "build": "dotnet build **/project.json",
    "webpack": "cd src/Gaver.Web && webpack",
    "cloc": "cloc --exclude-dir=node_modules,typings,wwwroot,bin,obj --exclude-lang=JSON ."
  },
  "author": "Geir Sagberg",
  "license": "MIT",
  "devDependencies": {
    "aspnet-webpack-react": "^1.0.2",
    "autoprefixer": "^6.4.1",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-3": "^6.11.0",
    "bootstrap": "^3.3.7",
    "css-loader": "^0.25.0",
    "del-cli": "^0.2.0",
    "eslint": "^3.4.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.2.0",
    "eslint-plugin-standard": "^2.0.0",
    "expose-loader": "^0.7.1",
    "extendify": "^1.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "foreach-cli": "^1.6.1",
    "postcss-loader": "^0.13.0",
    "postcss-nested": "^1.0.0",
    "postcss-simple-vars": "^3.0.0",
    "postcss-smart-import": "^0.5.0",
    "precss": "^1.4.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "watch-cli": "^0.2.1",
    "webpack": "^1.13.2",
    "webpack-hot-middleware": "^2.12.2"
  },
