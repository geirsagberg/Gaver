{
  "name": "gaver",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "foreach -g 'test/**Tests/*.csproj' -x 'cd #{dir} && dotnet test' --no-C",
    "test:watch": "npm test && chokidar '**/*.cs' -c 'npm test'",
    "clean": "del-cli src/**/obj/ src/**/bin/ test/**/obj/ test/**/bin/",
    "clean:www": "del-cli src/Gaver.Web/wwwroot/dist/",
    "start": "cd src/Gaver.Web && cross-env ASPNETCORE_ENVIRONMENT=Development dotnet run",
    "start:prod": "cd src/Gaver.Web && webpack && dotnet run",
    "watch": "cd src/Gaver.Web && cross-env ASPNETCORE_ENVIRONMENT=Development dotnet watch run",
    "build": "dotnet build",
    "webpack": "cd src/Gaver.Web && webpack",
    "webpack:dev": "cd src/Gaver.Web && cross-env ASPNETCORE_ENVIRONMENT=Development webpack",
    "cloc": "cloc --exclude-dir=node_modules,typings,wwwroot,bin,obj --exclude-lang=JSON ."
  },
  "author": "Geir Sagberg",
  "license": "MIT",
  "devDependencies": {
    "@types/lodash-es": "^4.17.0",
