{
  "parser": "babel-eslint",
  "extends": [ "standard", "plugin:react/recommended" ],
  "parserOptions": {
    "ecmaFeatures": {
      "modules": true
    }
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "eol-last": 0,
    "no-unused-vars": 1,
    "comma-dangle": 0,
    "space-before-function-paren": 1,
    "no-trailing-spaces": [
      2,
      {
        "skipBlankLines": true
      }
    ],
    "prefer-const": 1,
    "no-use-before-define": 1,
    "no-unused-expressions": 1,
    "react/prop-types": 0
  },
  "plugins": [ "react" ],
  "globals": {
    "fetch": false
  }
}
